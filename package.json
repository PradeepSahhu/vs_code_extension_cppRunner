{
  "name": "c-c---code-runner-for-mac",
  "displayName": "c/c++ code runner for mac",
  "description": "Sometimes latest c/c++ code doesn't work on macbooks hence create this to execute the latest c/c++ code in visual studio code. ",
  "publisher": "MacCodeRunner",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    
   
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "c-c---code-runner-for-mac.c_c++CodeRunner",
      "title": "run it"
      
    },
    {
      "command":"c-c---code-runner-for-mac.c_c++CodeDelete",
      "title": "delete it"
    },
    {
      "command":"c-c---code-runner-for-mac.c_c++CodeCopy",
      "title": "copy it"
    }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "icon": "./cimage.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mubashar-javed/quick-snippets"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
